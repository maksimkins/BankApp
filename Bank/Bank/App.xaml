             xmlns:local="clr-namespace:Bank"
             xmlns:vm="clr-namespace:Bank.ViewModels"
             xmlns:v="clr-namespace:Bank.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        
        <DataTemplate DataType="{x:Type vm:HomeViewModel}">
            <v:HomeView/>
        </DataTemplate>
        
    </Application.Resources>
</Application>
